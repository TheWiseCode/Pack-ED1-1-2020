#include <iostream>
#include <string>
#include "PilaSm.h"
#include "UMemoria.h"

using namespace std;

int main() {
	string cad = "(3x)*(4y)+(3d)/(2d))";
	PilaSm* pila = new PilaSm();
	for (int i = 0; i < cad.length(); i++) {
		int s;
		char c = cad[i];
		if (c == '(')
			pila->meter(c);
		else if (c == ')') {
			if (pila->vacia()) {
			}
			pila->sacar(s);
		}
	}
	if (pila->vacia())
		cout << "Parentesis correctos\n";
	else
		cout << "Parentesis incorrectos\n";
	system("pause");
	return 0;
}
