#pragma hdrstop

#include "ColaPila.h"
// ---------------------------------------------------------------------------
#pragma package(smart_init)

ColaPila::ColaPila() {
	p = new PilaPtr();
}

bool ColaPila::vacia() {
	return p->vacia();
}

void ColaPila::poner(int e) {
	PilaPtr* aux = new PilaPtr();
	while (!p->vacia()) {
		int e;
		p->sacar(e);
		aux->meter(e);
	}
	p->meter(e);
}

void ColaPila::sacar(int &e) {
}

int ColaPila::primero() {
}

string ColaPila::toStr() {

}
